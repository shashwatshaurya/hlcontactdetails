{
  "name": "hlcontactdetails",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18.18.0",
    "yarn": ">=1.22.0"
  },
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "analyze": "vite build --mode analyze",
